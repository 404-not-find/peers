<peers xmlns="http://peers.sourceforge.net"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://peers.sourceforge.net peers.xsd">
  <network>
    <interfaces>
      <interface id="eth1">
        <name>Ethernet network interface</name>
        <!-- uncomment the following element to specify the ip address you
             want to use -->
        <!--
        <address>192.168.1.20</address>
        -->
      </interface>
    </interfaces>
  </network>
  <devices>
    <!-- uncomment the following option to deactivate media playback and
         caputre -->
    <!--
    <media>false</media>
    -->
    <audio></audio>
    <video></video>
  </devices>
  <sip>
    <profile>
      <!-- username -->
      <userpart>8155</userpart>
      <!-- domain registrar ip address -->
      <domain>nexcom.fr</domain>
      <!-- if password is commented out, no REGISTER message is sent -->
      <password>1234</password>
      <!-- uncomment the following option to provide an outbound proxy.
           Don't forget the ;lr parameter to use loose routing, strict routing
           is not supported. -->
      <outboundProxy>sip:192.168.2.216;lr</outboundProxy>
      <interface ref="eth1"/>
      <!-- you can specify the sip listening port you want to use -->
      <port>6060</port>
    </profile>
  </sip>
  <codecs>
    <codec>
      <family>audio</family>
      <name>PCMU</name>
      <payloadType>0</payloadType>
      <!--
      <attributes>
        <attribute name="ptime" value="20"/>
      </attributes>
      -->
    </codec>
  </codecs>
  <rtp>
    <interface ref="eth1"/>
    <port>6002</port>
  </rtp>
</peers>
