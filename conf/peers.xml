<?xml version="1.0"?>

<peers xmlns="http://peers.sourceforge.net"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://peers.sourceforge.net peers.xsd">
  <network>
    <interfaces>
      <interface id="eth1">
        <name>Ethernet network interface</name>
        <!-- you can specify the ip address you want
        <address>192.168.1.20</address>
        -->
      </interface>
    </interfaces>
  </network>
  <devices>
    <audio></audio>
    <video></video>
  </devices>
  <sip>
    <profile>
      <userpart>alice</userpart>
      <domain>atlanta.com</domain>
      <!-- not implemented
      <outboundProxy>
        <address>192.168.1.10</address>
        <port>5060</port>
      </outboundProxy>
      -->
      <interface ref="eth1"/>
      <!-- you can specify the sip listening port you want
      <port>6060</port>
      -->
    </profile>
  </sip>
  <codecs>
    <codec>
      <family>audio</family>
      <name>PCMU</name>
      <payloadType>0</payloadType>
      <!--
      <attributes>
        <attribute name="ptime" value="20"/>
      </attributes>
      -->
    </codec>
  </codecs>
  <rtp>
    <interface ref="eth1"/>
    <port>6002</port>
  </rtp>
</peers>
